@using AvibaWeb.ViewModels.DataViewModels
@model OrganizationFinancialAccountsViewModel

<div>
    <select data-placeholder="Выбрать корпоратора" id="selectPayer" data-rel="chosen">
        <option value=""></option>
        <optgroup label="Корпораторы">
            @foreach (var counterparty in Model.Accounts)
            {
            <option id=@counterparty.Key>@counterparty.Value</option>
            }
        </optgroup>
    </select>
</div>