@using AvibaWeb.ViewModels.ExpenditureViewModels
@model ExpenditureSummaryOperationsViewModel


    <table class="table table-striped table-bordered bootstrap-datatable datatable responsive">
        <thead>
            <tr>
                <th>Дата</th>
                <th>Сумма</th>
                <th>Тип оплаты</th>
            </tr>
        </thead>
        <tbody>
            @foreach (ExpenditureSummaryOperationsItem i in Model.Items)
            {
            <tr>
                <td>@i.OperationDateTime</td>
                <td>
                    @i.Amount
                </td>
                <td>
                    @i.PaymentType
                </td>
            </tr>
            }
        </tbody>
    </table>

