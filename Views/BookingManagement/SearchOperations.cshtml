<br />
<div>
    <select id="searchSelect">
        <option value="1">Номер билета</option>
        <option value="2">ФИО</option>
        <option value="3">PNR</option>
        <option value="4">Паспорт</option>
    </select>
    <input type="text" id="keyString" autocomplete="off" />
    <button id="searchButton">Найти</button>
</div>

<div id="operationsResult" />

<script>
    $(function() {
        //$(".datePicker").datepicker({
        //    autoclose: true
        //}).datepicker('setDate', moment().format("DD.MM.YYYY"));
    });

    $("#searchButton").click(function (e) {
        $.ajax({
            url: "/BookingManagement/SearchOperations",
            type: "POST",
            cache: false,
            data: {
                key: $('#keyString').val(),
                type: $('#searchSelect').val()
            },
            dataType: "json",
            success: function (result) {
                $('#operationsResult').html(result.message);
            },
            error: function (error) {
                console.log(error.message);
            }
        });
    });
</script>